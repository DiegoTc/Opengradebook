<div id="content-header">
  <%= image_tag("/images/examination/show_exam.png") %>
  <h1><%= t('edit')  %> - <%= @student_grade_personality.student_additional_grade_field.name if  @student_grade_personality %></h1>
  <h3 style="text-transform:capitalize"><%= @student.full_name %></h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>


<div id="page-yield">
  <% unless flash[:notice].nil? %>
      <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>

  <div id="application-list">
        <% form_for(@student_grade_personality) do |f| %>
          <%= f.error_messages %>
            <table id="listing" align="center" width="100%" cellpadding="0" cellspacing="0">
              <tr class="tr-head">
                <td><%= f.label :p1 %></td>
                <td><%= f.label :p2 %></td>
                <td><%= f.label :p3 %></td>
                <td><%= f.label :p4 %></td>
                <td><%= t('actions') %></td>
              </tr>
              <tr class="tr-read-even">
                <td class="col-0"><%= f.text_field :p1, :size => 2 %></td>
                <td class="col-0"><%= f.text_field :p2, :size => 2 %></td>
                <td class="col-0"><%= f.text_field :p3, :size => 2 %></td>
                <td class="col-0"><%= f.text_field :p4, :size => 2 %></td>
                <td><%= f.submit t('update') %></td>
              </tr>
            </table>
        <% end %>
    </div>
  </div>