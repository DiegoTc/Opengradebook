<h1>Listing biweekly_subject_grades</h1>

<table>
  <tr>
    <th>Student</th>
    <th>Subject</th>
    <th>W1</th>
    <th>W2</th>
    <th>W3</th>
    <th>W4</th>
    <th>Period</th>
  </tr>

<% @biweekly_subject_grades.each do |biweekly_subject_grade| %>
  <tr>
    <td><%=h biweekly_subject_grade.student_id %></td>
    <td><%=h biweekly_subject_grade.subject_id %></td>
    <td><%=h biweekly_subject_grade.w1 %></td>
    <td><%=h biweekly_subject_grade.w2 %></td>
    <td><%=h biweekly_subject_grade.w3 %></td>
    <td><%=h biweekly_subject_grade.w4 %></td>
    <td><%=h biweekly_subject_grade.period %></td>
    <td><%= link_to 'Show', biweekly_subject_grade %></td>
    <td><%= link_to 'Edit', edit_biweekly_subject_grade_path(biweekly_subject_grade) %></td>
    <td><%= link_to 'Destroy', biweekly_subject_grade, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New biweekly_subject_grade', new_biweekly_subject_grade_path %>